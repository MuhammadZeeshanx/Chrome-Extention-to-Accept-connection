{
    "manifest_version": 3,
    "name": "LinkedIn Connection Acceptor",
    "version": "1.0",
    "description": "Automatically accept LinkedIn connection requests.",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["https://www.linkedin.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  